<?xml version="1.0" encoding="UTF-8"?>
<beans 
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
							http://www.springframework.org/schema/beans/spring-beans.xsd" >
	<bean id="officeCategory" class="ru.seyseich.model.Category" >
		<property name="name" value="Office" />
	</bean>
	
	<bean id="gadgetsCategory" class="ru.seyseich.model.Category" >
		<property name="name" value="Gadgets" />
	</bean>
	
	<bean id="penGoods" class="ru.seyseich.model.Goods" >
		<property name="name" value="Pens" />
		<property name="quantity" value="100" />
		<property name="price" value="5.00" />
		<property name="category" ref="officeCategory" />
	</bean>
	
	<bean id="pencilGoods" class="ru.seyseich.model.Goods" >
		<property name="name" value="Pencils" />
		<property name="quantity" value="55" />
		<property name="price" value="10.00" />
		<property name="category" ref="officeCategory" />
	</bean>
	
	<bean id="iphoneGoods" class="ru.seyseich.model.Goods" >
		<property name="name" value="iPhones" />
		<property name="quantity" value="10" />
		<property name="price" value="25000.00" />
		<property name="category" ref="gadgetsCategory" />
	</bean>
	
	<bean id="nexusGoods" class="ru.seyseich.model.Goods" >
		<property name="name" value="Nexuses" />
		<property name="quantity" value="12" />
		<property name="price" value="11000.00" />
		<property name="category" ref="gadgetsCategory" />
	</bean>
	
	<bean id="myWarehouse" class="ru.seyseich.model.Warehouse" >
		<property name="goods">
			<set>
				<ref bean="penGoods" />
				<ref bean="pencilGoods" />
				<ref bean="iphoneGoods" />
				<ref bean="nexusGoods" />
			</set>
		</property>
	</bean>
</beans>
